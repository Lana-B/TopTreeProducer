<k type="TRootPFJet" name="PFJets_selectedPatJetsAK5PF" minPt="15" maxEta="2.4" skipObjects="1" minNObjects="0"/>
<k type="TRootGenJet" name="GenJets" minPt="15" maxEta="2.4" skipObjects="1" minNObjects="0"/>
<k type="TRootGenEvent" name="GenEvent" minPt="0" maxEta="9999" skipObjects="0" minNObjects="0"/>
